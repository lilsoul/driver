TARGETNAME=plpolicy
TARGETTYPE=DRIVER
TARGET_DESTINATION=powertests\powerlimit\policy

# Specifies the file is needed for boot
# and for performance issues must have
# an embedded signature.
## MBS Team 2/5/2014
## Removing boot_loader_critical, instead doing kit_self_signed, because you have test_code=1
BOOT_LOADER_CRITICAL=1

NO_PDB_PATHS=1

MSC_WARNING_LEVEL=/W4 /WX

KMDF_VERSION_MAJOR=1

C_DEFINES= $(C_DEFINES) -DUNICODE -D_UNICODE

INCLUDES=$(INCLUDES); \
         $(MINWIN_PRIV_SDK_INC_PATH); \
         $(MINWIN_PRIV_SDK_INC_PATH)\hals; \
         ..\inc

SOURCES= \
        plpolicy.c \
        plpolicy.rc \
        wdf.c \

PASS0_BINPLACE=\
        plpolicy.inf \

MUI_VERIFY_NO_LOC_RESOURCE=1
